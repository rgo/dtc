<h1>Listing washing_machines</h1>

<table>
  <tr>
    <th>Producer</th>
    <th>Product</th>
    <th>Brand</th>
    <th>Model</th>
    <th>Rpm</th>
    <th>Efficiency</th>
    <th>Consume</th>
    <th>Washing efficiency</th>
    <th>Spin efficiency</th>
    <th>Capacity</th>
    <th>Consume kg</th>
    <th>Consume water</th>
    <th>Height</th>
    <th>Width</th>
    <th>Deep</th>
    <th>Termoefficiency</th>
  </tr>

<% @washing_machines.each do |washing_machine| %>
  <tr>
    <td><%=h washing_machine.producer %></td>
    <td><%=h washing_machine.product %></td>
    <td><%=h washing_machine.brand %></td>
    <td><%=h washing_machine.model %></td>
    <td><%=h washing_machine.rpm %></td>
    <td><%=h washing_machine.efficiency %></td>
    <td><%=h washing_machine.consume %></td>
    <td><%=h washing_machine.washing_efficiency %></td>
    <td><%=h washing_machine.spin_efficiency %></td>
    <td><%=h washing_machine.capacity %></td>
    <td><%=h washing_machine.consume_kg %></td>
    <td><%=h washing_machine.consume_water %></td>
    <td><%=h washing_machine.height %></td>
    <td><%=h washing_machine.width %></td>
    <td><%=h washing_machine.deep %></td>
    <td><%=h washing_machine.termoefficiency %></td>
    <td><%= link_to 'Show', washing_machine %></td>
    <td><%= link_to 'Edit', edit_washing_machine_path(washing_machine) %></td>
    <td><%= link_to 'Destroy', washing_machine, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New washing_machine', new_washing_machine_path %>